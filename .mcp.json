{
  "mcpServers": {
    "dbt-mcp": {
      "command": "bash",
      "args": ["scripts/launch-dbt-mcp.sh"],
      "disabled": false,
      "_comment": "Week 1 Day 5 - Using wrapper script to inherit 1Password env vars. Python 3.12 due to asyncio stdio bug in Python 3.13."
    },
    "snowflake-mcp": {
      "command": "bash",
      "args": ["scripts/launch-snowflake-mcp.sh"],
      "env": {
        "SNOWFLAKE_PASSWORD": "${SNOWFLAKE_PASSWORD}"
      },
      "disabled": false,
      "_comment": "Week 1 Day 5 - Using wrapper script to inject password into config at runtime. Static values hardcoded in config, only password injected."
    },
    "aws-api": {
      "command": "uvx",
      "args": ["awslabs.aws-api-mcp-server@latest"],
      "env": {
        "AWS_REGION": "us-west-2",
        "AWS_PROFILE": "default",
        "READ_OPERATIONS_ONLY": "true",
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "aws-docs": {
      "command": "uvx",
      "args": ["awslabs.aws-documentation-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      },
      "disabled": false,
      "autoApprove": []
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "disabled": false,
      "autoApprove": [],
      "_comment": "Week 1 Day 3 - ENABLED with GitHub PAT from 1Password (scopes: repo, read:org, read:project)"
    },
    "slack": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-slack"],
      "env": {
        "SLACK_BOT_TOKEN": "${SLACK_BOT_TOKEN}",
        "SLACK_TEAM_ID": "${SLACK_TEAM_ID}"
      },
      "disabled": false,
      "autoApprove": [],
      "_comment": "Week 1 Day 4 - ENABLED with Slack bot token from 1Password (scopes: channels:read, chat:write, users:read)"
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/TehFiestyGoat/da-agent-hub"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "disabled": false,
      "autoApprove": []
    }
  }
}